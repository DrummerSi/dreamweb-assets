{
	"compilerOptions": {
	  "module": "commonjs",
	  "esModuleInterop": true,
	  "target": "es2017",
	  "noImplicitAny": false,
	  "moduleResolution": "node",
	  "sourceMap": true,
	  "strict": true,
	  "noFallthroughCasesInSwitch": true,
	  "noImplicitReturns": true,
	  "noUnusedLocals": true,
	  "outDir": "dist",
	  "baseUrl": ".",
	  "paths": {
		"*": [
		  "node_modules/*",
		]
	  }
	},
	"include": [
	  "src/**/*"
	],
	"exclude": [
	  "node_modules",
	  "dist"
	]
  }